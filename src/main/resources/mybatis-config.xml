<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="sql.pojo.Suppliers" alias="Suppliers" />
		<typeAlias type="sql.pojo.SuppliersHistory" alias="SuppliersHistory" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="oracle.jdbc.OracleDriver" />
				<property name="url" value="jdbc:oracle:thin:@localhost:1521:TC10" />
				<property name="username" value="HOZON" />
				<property name="password" value="HOZON" />
			</dataSource>
		</environment>
	</environments>
	<!--映射文件：描述某个实体和数据库表的对应关系 -->
	<mappers>
		<mapper resource="sql/mapper/xmapper/SuppliersMapper.xml" />
		<mapper resource="sql/mapper/xmapper/SuppliersHistoryMapper.xml" />
		<mapper resource="sql/mapper/xmapper/GrantUserMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzReleaseRecordMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzReleaseFileRecordMapper.xml" />
		<mapper resource="sql/mapper/xmapper/UserAuthorizationMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzAuthorityOfuserMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzTempMainRecordMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzTempItemRecordMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzTempDocumentRecordMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzBomMainRecordMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzBomLineRecordMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzPreferenceSettingMapper.xml" />
		<mapper resource="sql/mapper/xmapper/HzBom2YEntityMapper.xml" />
	</mappers>
</configuration>